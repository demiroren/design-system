{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/stories"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "isolatedModules": true,
    "allowJs": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
